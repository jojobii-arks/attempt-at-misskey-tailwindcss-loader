{
  "name": "tailwindcss-misskey-theme",
  "version": "0.0.0",
  "type": "module",
  "description": "Misskey Theme loader for Tailwind CSS", 
  "license": "MIT",
  "packageManager": "pnpm@8.1.1",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
